                        <StackPanel HorizontalAlignment="Left" Orientation="Horizontal" VerticalAlignment="Top" Width="351">
                            <TextBlock Width="219" FontSize="12" Margin="10,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Center"  x:Uid="FotballtippingSonMatchesLabel" />
                            <TextBlock Width="50" FontSize="12" VerticalAlignment="Top" HorizontalAlignment="Center"  Margin="10,10,0,0" TextAlignment="Center"  x:Uid="FotballtippingSonHalftimeLabel" />
                            <TextBlock Width="50" FontSize="12" VerticalAlignment="Top" HorizontalAlignment="Center"  Margin="10,10,0,0" TextAlignment="Center"  x:Uid="FotballtippingSonFulltimeLabel" />
                        </StackPanel>
                        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Top" Orientation="Horizontal" Width="Auto" Height="255">
                            <TextBlock x:Name="FotballtippingSonVinnertallValues"  Margin="10,10,0,-21.333" TextWrapping="Wrap" Text="{Binding Vinnertall}" VerticalAlignment="Top" Height="243" Width="Auto" FontSize="12" RenderTransformOrigin="0.5,0.5"/>
                            <TextBlock x:Name="FotballtippingSonTilleggstallValues"  Margin="10,10,0,-0.333" TextWrapping="Wrap" HorizontalAlignment="Center" Text="{Binding Tilleggstall}" VerticalAlignment="Top" Height="242" Width="50" FontSize="12" RenderTransformOrigin="0.322,0.395" TextAlignment="Center"/>
                            <TextBlock x:Name="FotballtippingSonHeltidstallValues"  Margin="10,10,0,-0.333" TextWrapping="Wrap" HorizontalAlignment="Center" Text="{Binding Spillnavn}" VerticalAlignment="Top" Height="242" Width="50" FontSize="12" RenderTransformOrigin="0.322,0.395" TextAlignment="Center"/>
                        </StackPanel>
                        <TextBlock x:Uid="FotballtippingSonStatusLabel"  HorizontalAlignment="Left" Margin="10,0,0,0" TextWrapping="Wrap" Text="{Binding Tilleggspremie}" VerticalAlignment="Top" Height="22" Width="341" FontSize="18"/>
                        <TextBlock x:Uid="FotballtippingSonPremieprognoseLabel" HorizontalAlignment="Left" Margin="10,0,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="42" Width="312" FontSize="36"/>
